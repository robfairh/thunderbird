boundary
{
    minY
    {
        type            patch;
        inGroups        (coupleGroup);
    }
}

T
{
    internalField   uniform 323;

    boundaryField
    {
        ".*"
        {
            type            zeroGradient;
        }

        heater_to_topAir
        {
            type            compressible::turbulentTemperatureCoupledBaffleMixed;
            Tnbr            T;
            kappaMethod     solidThermo;
            value           uniform 323;
        }

        minY
        {
            type            fixedValue;
            value           uniform 323;
        }
    }
}

