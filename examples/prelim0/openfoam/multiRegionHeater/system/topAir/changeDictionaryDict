boundary
{
    ".*"
    {
        //inGroups        (coupleGroup);       
        type            wall;
        inGroups        (wall);
    }

    topAir_to_heater
    {
        inGroups        (coupleGroup);
    }
}

U
{
    internalField   uniform (0 0 0);

    boundaryField
    {
        ".*"
        {
            type            fixedValue;
            value           uniform (0 0 0);
        }
    }
}

T
{
    internalField   uniform 273;

    boundaryField
    {
        ".*"
        {
            type            fixedValue;
            value           uniform 273;
        }

        topAir_to_heater
        {
            type            compressible::turbulentTemperatureCoupledBaffleMixed;
            Tnbr            T;
            kappaMethod     fluidThermo;
            value           uniform 273;
        }
    }
}

epsilon
{
    internalField   uniform 0.01;

    boundaryField
    {
        ".*"
        {
            type            epsilonWallFunction;
            value           uniform 0.01;
        }
    }
}

k
{
    internalField   uniform 0.1;

    boundaryField
    {
        ".*"
        {
            type            kqRWallFunction;
            value           uniform 0.1;
        }
    }
}

